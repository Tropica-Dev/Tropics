{
	"schemaVersion": 1,
	"id": "tropics",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_authors}"
	],
	"contact": {
		"homepage": "${homepage_url}",
		"sources": "${source_code_url}"
	},
	"license": "${mod_license}",
	"icon": "${mod_icon_path}",
	"environment": "*",
	"accessWidener": "${mod_id}.accesswidener",
	"entrypoints": {
		"main": [
			"${mod_entrypoint}"
		],
		"client": [
			"${mod_client_entrypoint}"
		],
		"data": [
			"${mod_datagen_entrypoint}"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json",
		"${mod_id}.client.mixins.json"
	],
	"depends": {
		"fabricloader": "${loader_version_range}",
		"fabric": "${fabric_api_version_range}",
		"minecraft": "${minecraft_version_range}",
		"geckolib": "${geckolib_version_range}",
		"java": ">=17"
	},
	"suggests": {
	},
	"custom": {
		"modmenu": {
			"links": {
				"tropics.discord": "https://discord.gg/2SQTytEZX2"
			},
			"parent": {
				"id": "${mod_id}",
				"name": "${mod_name}",
				"description": "${mod_description}",
				"icon": "${mod_icon_path}"
			},
			"update_checker": true
		}
	}
}
